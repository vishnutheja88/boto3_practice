# get creation date from s3 bucket
#pip install boto3
#pip install awscli

import boto3
resource = boto3.resource("s3")
resource.list_buckets()["Buckets"][0]["Name"]
creation_date=resource.list_buckets()["BUckets"][0]["CreationDate"]
creation_date.strftime("%d%m%y_%H:%M:%s")

